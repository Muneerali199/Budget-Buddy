{
  "version": "15.50.00",
  "name": "budget-buddy",
  "alias": ["budget-buddy"],
  "regions": ["iad1", "sfo1"],
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}